[root]
size=4G
type=ext4
mountpoint=/
options=errors=remount-ro,relatime

[swap]
size=512M
type=swap

[var]
size=1G
type=ext4
mountpoint=/var
options=defaults,relatime

[var-log]
size=1G
type=ext4
mountpoint=/var/log
options=defaults,noatime

[tmp]
size=1G
type=ext4
mountpoint=/tmp
options=defaults,relatime

